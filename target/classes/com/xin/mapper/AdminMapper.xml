<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xin.mapper.AdminMapper">
	<!-- 登录 -->
	<select id="selectAdminByLoginnameAndLoginpass" resultType="com.xin.entity.Admin">
		select  *  from  `admin`
		where 
		adminname =#{adminname}
		and
		adminpwd = #{adminpwd}
	</select>

</mapper>